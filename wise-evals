#!/bin/bash
thisFile=$1
thisDir=`basename "$PWD"`
justFile=$(basename "$thisFile")
outFile=$justFile.html
:> "$outFile"
echo "<h1>$justFile</h1>" >> "$outFile"
echo "<h2>Evaluation results</h2>">> "$outFile"
csvstat --freq -c 2,3,4,5 "$thisFile" | sed 's/["{}]//g'| sed 's/$/\<br\/\>/g'  >> "$outFile"
echo "<h2>Median scores</h2>" >> "$outFile"
csvstat --median -c 6,7,8 "$thisFile" | sed 's/["{}]//g'| sed 's/$/\<br\/\>/g' >> "$outFile"
echo "<h2>Comments</h2>" >> "$outFile"
csvcut -c 9 "$thisFile" | sed '/""/d' | sed 's/^"/\<p\>"/g' | sed 's/$/\<br\/\>/g'  >> "$outFile"
echo "<hr/><p>" >> "$outFile"
csvcut -c 10 "$thisFile" | sed '/""/d' | sed 's/^"/\<p\>"/g' | sed 's/$/\<br\/\>/g' >> "$outFile"
echo "<hr/><p>" >> "$outFile"
csvcut -c 11 "$thisFile" | sed 's/""//g' | sed '/^$/d ' | sed 's/$/\<br\/\>/g' >> "$outFile"

